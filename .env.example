
# .env.example

# Telegram bot token
TOKEN=your-telegram-token-here

# Webhook port (default for dev: 8443)
PORT=8443

# Webhook URL will be injected during development
# WEBHOOK_URL=https://yourdomain.com/webhook

# PostgreSQL database connection string
DATABASE_URL=postgresql://botuser:password@host:port/dbname

API_KEY_HF=your-huggingface-api-key-here